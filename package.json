{
  "name": "auto-require",
  "version": "2.0.1",
  "description": "No need to require any packages now",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha",
    "build": "coffee -bco ./dist/ ./src/*.coffee",
    "dev": "coffee -bwco ./dist/ ./src/*.coffee",
    "pretty": "js-beautify ./dist/index.js --outfile ./dist/index.js --config ./jsbeautify.json",
    "deploy": "npm build && git push origin master & npm publish",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ViktorKonsta/auto-require.git"
  },
  "author": "viktor.konsta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViktorKonsta/auto-require/issues"
  },
  "keywords": "auto, require, auto-require, fast, automaticaly, easy, automatic, loader, auto-loader, module-loader, module",
  "dependencies": {
    "chalk": "^1.1.1",
    "lodash.zipobject": "^3.0.0"
  },
  "devDependencies": {
    "andyet-express-auth": "^0.4.1",
    "broccoli": "^0.16.8",
    "broccoli-file-contents-to-json": "^0.2.1",
    "broccoli-react-caching": "^0.1.1",
    "broccoli-sass": "^0.6.7",
    "broccoli-use-strict-remover": "^1.1.0",
    "browser-sync": "^2.9.6",
    "chai": "^3.3.0",
    "coffee-script": "^1.10.0",
    "express": "^4.13.3",
    "grunt": "^0.4.5",
    "grunt-closure-tools": "^0.9.9",
    "grunt-load": "^0.1.2",
    "gulp": "^3.9.0",
    "gulp-inline-css": "^2.0.0",
    "gulp-plumber": "^1.0.1",
    "run-sequence": "^1.1.4"
  },
  "homepage": "https://github.com/ViktorKonsta/auto-require#readme"
}
